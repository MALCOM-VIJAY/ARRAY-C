#include <stdio.h>

int main()
{
    int a[]={1,2,3,4,5,6,6};
    int len= sizeof(a)/sizeof(a[0]),sum=0;
    printf("The duplicate elements are: ");
    for(int i=0;i<len;i++){
        for(int j=i+1;j<len;j++){
            if(a[i]==a[j]){
                printf("%d\n",a[j]);
                sum+=1;
            }
        }
    }
    printf("Totally %d numbers are repeated",sum);
    return 0;
}
