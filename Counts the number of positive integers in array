#include <stdio.h>
int main()
{
    int a[]={1,2,-3,5,-6};
    int len = sizeof(a)/sizeof(a[0]),sum=0;
    for(int i=0;i<len;i++){
        printf("%d ",a[i]);
    }
    printf("\n");
    printf("Positive integers are: ");
    for(int i= 0;i<len;i++){
        if(a[i]>0){
            printf("%d ",a[i]);
            sum+=1;
        }
    }
    printf("\nTotal number of positive integers:%d ",sum);
    return 0;
}
